from github import Github
g = Github("fbf5c16d4943e1d768c7a55a2894b2acbbb0ebb3")
user = g.get_user()
print(user.login) #Get current user
print(user.name) #Get user by name
repor = g.get_repo("Rajat986/cse") #Get repository by name
print(repor.name) 
org = g.get_organization("PyGithub") #Get organization by name
print(org.login)
